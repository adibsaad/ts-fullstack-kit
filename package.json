{
  "name": "ts-fullstack-kit",
  "version": "0.1.0",
  "engines": {
    "node": "18"
  },
  "scripts": {
    "codegen": "graphql-codegen --config codegen.ts",
    "codegen:watch": "graphql-codegen --config codegen.ts -w",
    "install-all": "pnpm install && cd src/frontend && pnpm install",
    "build:cli": "webpack -c ./scripts/build_cli.ts",
    "build:frontend": "run-s lint:frontend frontend:build",
    "build:lambda": "webpack -c ./scripts/build-lambda.ts",
    "build:server": "webpack -c ./scripts/build_server.ts",
    "build:worker": "webpack -c ./scripts/build_worker.ts",
    "db:drop": "./scripts/db/drop.sh",
    "db:create": "./scripts/db/create.sh",
    "db:recreate": "./scripts/db/recreate.sh",
    "db:prisma:create_migration": "cd src/server && prisma migrate dev --create-only",
    "db:prisma:migrate": "cd src/server && prisma migrate dev",
    "db:prisma:generate": "cd src/server && prisma generate",
    "format": "prettier --write .",
    "frontend:build": "cd src/frontend && pnpm build",
    "lint": "eslint . --max-warnings=0",
    "lint:frontend": "cd src/frontend && pnpm lint",
    "start": "overmind s && sleep 1 && overmind echo",
    "start:cli": "npm-run-all --parallel start:watch-cli",
    "start:watch-cli": "webpack watch -c ./scripts/watch_cli.ts",
    "start:frontend": "cd src/frontend && pnpm dev",
    "start:server": "npm-run-all --parallel start:watch-server start:run-server",
    "start:watch-server": "webpack watch -c ./scripts/watch_server.ts",
    "start:run-server": "wait-on dist/server/index.js && nodemon --watch dist/server/index.js dist/server/index.js",
    "start:worker": "npm-run-all --parallel start:watch-worker start:run-worker",
    "start:watch-worker": "webpack watch -c ./scripts/watch_worker.ts",
    "start:run-worker": "wait-on dist/worker/index.js && nodemon --watch dist/worker/index.js dist/worker/index.js",
    "stop": "overmind kill",
    "test:server": "cd src/server && jest -w 2",
    "test:server:watch": "cd src/server && jest -w 2 --watch",
    "test:server:ci": "cd src/server && COVERAGE=1 jest -w 2",
    "test:frontend": "cd src/frontend && pnpm test",
    "test:frontend:ci": "cd src/frontend && pnpm test"
  },
  "dependencies": {
    "@0no-co/graphqlsp": "^1.12.16",
    "@aws-sdk/client-sqs": "^3.826.0",
    "@cjs-exporter/p-map": "^5.5.0",
    "@fastify/aws-lambda": "^5.1.4",
    "@fastify/cors": "^9.0.1",
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/schema-ast": "^4.1.0",
    "@graphql-codegen/typescript-react-apollo": "^4.3.3",
    "@paddle/paddle-node-sdk": "^1.10.0",
    "@pothos/core": "^4.6.2",
    "@pothos/plugin-errors": "^4.3.0",
    "@pothos/plugin-prisma": "^4.8.2",
    "@pothos/plugin-scope-auth": "^4.1.3",
    "@prisma/client": "5.19.1",
    "@sentry/aws-serverless": "^8.55.0",
    "@sentry/node": "^8.55.0",
    "@sentry/serverless": "^7.120.3",
    "@sentry/webpack-plugin": "^2.23.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/aws-lambda": "^8.10.149",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/mjml": "^4.7.4",
    "@types/mustache": "^4.2.6",
    "@types/node": "^22.15.31",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.17",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "clean-webpack-plugin": "^4.0.0",
    "commander": "^12.1.0",
    "copy-webpack-plugin": "^12.0.2",
    "esbuild-loader": "^4.3.0",
    "eslint": "^8.57.1",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^3.10.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.13.3",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "eslint-plugin-unused-imports": "^3.2.0",
    "fastify": "^4.29.1",
    "fork-ts-checker-webpack-plugin": "^9.1.0",
    "google-auth-library": "^9.15.1",
    "graphql": "^16.11.0",
    "graphql-yoga": "^5.13.5",
    "html-loader": "^5.1.0",
    "html-minimizer-webpack-plugin": "^5.0.2",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.2",
    "jest-fetch-mock": "^3.0.3",
    "jest-html-loader": "^1.0.0",
    "jest-transform-stub": "^2.0.0",
    "jest-when": "^3.7.0",
    "jsonwebtoken": "^9.0.2",
    "mjml": "^4.15.3",
    "mustache": "^4.2.0",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.10.1",
    "nodemailer-postmark-transport": "^6.0.1",
    "nodemon": "^3.1.10",
    "npm-run-all": "^4.1.5",
    "null-loader": "^4.0.1",
    "pino": "^9.7.0",
    "pino-pretty": "^11.3.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.12",
    "prisma": "^5.22.0",
    "sqs-consumer": "^11.6.0",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "uuid": "^10.0.0",
    "wait-on": "^8.0.3",
    "webpack": "^5.99.9",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0",
    "zip-webpack-plugin": "^4.0.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "@sentry/cli",
      "@swc/core",
      "esbuild",
      "prisma",
      "unrs-resolver"
    ]
  }
}
